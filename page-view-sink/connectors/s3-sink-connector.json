{
  "name": "page-view-s3-sink",
  "config": {
    "connector.class": "io.confluent.connect.s3.S3SinkConnector",
    "tasks.max": "1",
    "topics": "test.streaming.page-view.output",
    "s3.region": "us-east-1",
    "s3.bucket.name": "test-page-view-sink-output",
    "storage.class": "io.confluent.connect.s3.storage.S3Storage",
    "format.class": "io.confluent.connect.s3.format.json.JsonFormat",
    "flush.size": "1",
    "rotate.interval.ms": "60000",
    "partitioner.class": "io.confluent.connect.storage.partitioner.TimeBasedPartitioner",
    "path.format": "'year'=YYYY/'month'=MM/'day'=dd/'hour'=HH/'minute'=mm",
    "locale": "en-US",
    "timezone": "UTC",
    "partition.duration.ms": "60000",
    "timestamp.extractor": "RecordField",
    "timestamp.field": "aggregationWindow",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "https://psrc-z6m5xyw.us-east-1.aws.confluent.cloud",
    "value.converter.basic.auth.credentials.source": "USER_INFO",
    "value.converter.basic.auth.user.info": "${SCHEMA_REGISTRY_USER_INFO}"
  }
}